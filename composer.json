{
  "name": "chamber-orchestra/doctrine-sort-bundle",
  "type": "library",
  "description": "The symfony doctrine sort bundle",
  "keywords": [
    "symfony",
    "doctrine-sort-bundle",
    "doctrine-sort",
    "sort",
    "doctrine"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Lukin",
      "email": "lukin.andrej@gmail.com",
      "homepage": "https://github.com/wtorsi",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "chamber-orchestra/metadata-bundle": "8.0.*",
    "php-ds/php-ds": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5",
    "symfony/test-pack": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "ChamberOrchestra\\DoctrineSortBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "8.0.*"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
